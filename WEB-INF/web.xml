<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <!-- Student Login Servlet -->
    <servlet>
        <servlet-name>StudentLoginServlet</servlet-name>
        <servlet-class>StudentLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StudentLoginServlet</servlet-name>
        <url-pattern>/StudentLoginServlet</url-pattern>
    </servlet-mapping>

    <!-- Faculty Login Servlet -->
    <servlet>
        <servlet-name>FacultyLoginServlet</servlet-name>
        <servlet-class>FacultyLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FacultyLoginServlet</servlet-name>
        <url-pattern>/FacultyLoginServlet</url-pattern>
    </servlet-mapping>

    <!-- Submit Feedback Servlet -->
<servlet>
    <servlet-name>SubmitFeedbackServlet</servlet-name>
    <servlet-class>SubmitFeedbackServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>SubmitFeedbackServlet</servlet-name>
    <url-pattern>/SubmitFeedbackServlet</url-pattern>
</servlet-mapping>


</web-app>
